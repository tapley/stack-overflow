<h1>Questions Index</h1>
<%= render 'new_form' %>
<% @questions.each do |question| %>
<h3><%= link_to question.title, question, method: :GET %></h3>
<div><%= link_to "Up", question, method: :GET, vote: {value: 1, votable_id: question.id, votable_type: question} %></div>
<div><%= link_to "Down", question, method: :GET %></div>
<content><%= question.content %></content>
<%end %>
